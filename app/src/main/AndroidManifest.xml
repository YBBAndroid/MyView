<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ybb.testview">
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name=".MainActivity"
                  android:theme="@style/AppTheme"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>


        <activity android:name=".View.MenuView.MenuActivity"/>

        <activity android:name=".View.PaintView.PaintViewActivity"/>
        <activity android:name=".View.WareView.WareViewActivity"/>

        <activity android:name=".View.FloatingView.FloatingViewActivity"/>

        <activity android:name=".RecylerView.ChatUI.ChatActivity"/>

        <activity android:name=".View.RuningTextView.RuningTextActivity"/>



    </application>

</manifest>